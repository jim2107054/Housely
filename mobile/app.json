{
  "expo": {
    "name": "Housely",
    "slug": "housely",
    "version": "1.0.0",
    "scheme": "housely",

    "web": {
      "favicon": "./assets/favicon.png"
    },

    "experiments": {
      "tsconfigPaths": true,
      "typedRoutes": true
    },

    "plugins": [
      "expo-router"
    ],

    "orientation": "portrait",
    "icon": "./assets/images/Logo.png",

    "userInterfaceStyle": "light",

    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.housely.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.housely.app"
    }
  }
}
